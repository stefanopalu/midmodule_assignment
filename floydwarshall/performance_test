"""compare performance of the two algorythms"""

import timeit
from recursive_floydwarshall import floydwarshall_recursive
from floydwarshall_alg import floydWarshall

INF = 99999

my_matrix = [[0, 3, INF, 5],[2, 0, INF, 4],[INF, 1, 0, INF],[INF, INF, 2, 0]]


recstart = timeit.default_timer()
print("The start time is :", recstart)
floydwarshall_recursive(my_matrix)
print("The difference of time is :", timeit.default_timer() - recstart)
start = timeit.default_timer()
print("The start time is :", start)
floydWarshall(my_matrix)
print("The difference of time is :", timeit.default_timer() - start)
